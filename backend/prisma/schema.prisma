generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Instrument {
     TRUMPET
     BASS
     SAX
     CLARINET
     TROMBONE
     FLUTE
     HORN
     MAJORETTE
}



model Users {
   userId  Int @id @default(autoincrement())
   name    String
   email   String @unique
   instrument Instrument
   password String
   create_at DateTime @default(now())
   update_at DateTime @updatedAt
}


   
